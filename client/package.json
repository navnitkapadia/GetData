    "nouislider": "13.1.0",
    "paho-mqtt": "^1.1.0",
    "prop-types": "15.7.1",
    "react": "^16.8.3",
    "react-apexcharts": "^1.3.0",
    "react-chartjs-2": "^2.7.4",
    "react-dom": "^16.8.3",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^5.0.1",
